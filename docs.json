{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Maneged",
  "colors": {
    "primary": "#fddb6b",
    "light": "#fddb6b",
    "dark": "#fddb6b"
  },
  "appearance":{
    "default": "dark",
    "strict": true
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "architecture"
            ]
          },
          {
            "group": "Backend Server",
            "pages": [
              "backend/introduction",
              "backend/installation",
              "backend/authentication",
              "backend/websocket"
            ]
          },
          {
            "group": "Mobile App",
            "pages": [
              "mobile/introduction",
              "mobile/installation",
              "mobile/architecture",
              "mobile/features"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "REST API",
            "pages": [
              "api-reference/introduction",
              "api-reference/authentication"
            ]
          },
          {
            "group": "User Management",
            "pages": [
              "api-reference/users/get-profile",
              "api-reference/users/update-profile",
              "api-reference/users/fcm-tokens"
            ]
          },
          {
            "group": "Organizations",
            "pages": [
              "api-reference/organizations/create",
              "api-reference/organizations/join-request",
              "api-reference/organizations/search",
              "api-reference/organizations/members"
            ]
          },
          {
            "group": "Conversations",
            "pages": [
              "api-reference/conversations/list",
              "api-reference/conversations/create",
              "api-reference/conversations/get"
            ]
          },
          {
            "group": "Messages",
            "pages": [
              "api-reference/messages/send",
              "api-reference/messages/list",
              "api-reference/messages/reactions"
            ]
          },
          {
            "group": "Tasks",
            "pages": [
              "api-reference/tasks/create",
              "api-reference/tasks/update",
              "api-reference/tasks/list"
            ]
          },
          {
            "group": "WebSocket Events",
            "pages": [
              "api-reference/websocket/connection",
              "api-reference/websocket/presence",
              "api-reference/websocket/messages"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/manegedtech",
          "icon": "github"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@maneged.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@maneged.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
